{
  "name": "@closedai/sdk",
  "version": "1.0.0",
  "description": "CLOSED AI TypeScript SDK and CLI for generating React components from flows",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "closedai": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "closedai",
    "automation",
    "ai",
    "flows",
    "react",
    "typescript"
  ],
  "author": "CLOSED AI Team",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.0",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "fs-extra": "^11.0.0",
    "handlebars": "^4.7.8",
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "files": [
    "dist/**/*",
    "templates/**/*"
  ]
} 